  "scripts": {
    "start": "node ./build/index.js",
    "start-dev": "nodemon ./build/index.js",
    "build:client": "webpack --config webpack.config.client.ts --mode production",
    "build:server": "webpack --config webpack.config.server.ts --mode production",
    "build": "concurrently \"yarn build:client\" \"yarn build:server\"",
    "watch:client": "webpack --config webpack.config.client.ts --mode development -w",
    "watch:server": "webpack --config webpack.config.server.ts --mode development -w",
    "watch": "concurrently \"yarn watch:client\" \"yarn watch:server\"",
    "lint": "tslint --project tsconfig.json",
    "test": "jest --collectCoverage",
    "test:watch": "jest --watch --collectCoverage"
  },
